            /* Composed */
            ),
            cancelable: !!(flags & 1
            /* Cancellable */
            ),
            detail: detail
          }));
        }
      };
    };

    var getAssetPath = function getAssetPath(path) {
      var assetUrl = new URL(path, plt.$resourcesUrl$);
      return assetUrl.origin !== win.location.origin ? assetUrl.href : assetUrl.pathname;
    };

