    search() {
        console.log(this.categories);
        console.log(this.cities);
        let val = this.searchTerm;
        if (val.trim() == '') {
            if (this.categories.length > 0 && this.cities.length > 0) {
                console.log(Array.isArray(this.categories));
                console.log(Array.isArray(this.cities));
