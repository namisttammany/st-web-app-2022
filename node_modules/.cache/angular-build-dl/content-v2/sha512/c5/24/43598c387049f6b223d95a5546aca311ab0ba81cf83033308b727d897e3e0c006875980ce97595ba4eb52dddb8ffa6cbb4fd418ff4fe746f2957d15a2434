let ResourceListService = class ResourceListService {
    constructor(db) {
        this.db = db;
        this.resourceListRef = this.db.list('resource', ref => ref.orderByChild('ProgramName'));
        this.cityList = this.db.list('cities', ref => ref.orderByChild('city'));
        this.categoryList = this.db.list('categories', ref => ref.orderByChild('category'));
    }
    getResourceList() {
        return this.resourceListRef;
    }
    getCityList() {
        return this.cityList;
    }
    getCategoryList() {
        return this.categoryList;
    }
};
