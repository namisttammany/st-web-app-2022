()\n    .then((res) => {\n      this.router.navigate(['/home']);\n    })\n  }\n\n\n  goToHomePage(params){\n    this.router.navigate(['/home'])\n  }\n\n  goRegisterPage(){\n      this.router.navigate(['/register']);\n    }\n\n  goToResetPasswordPage(params){\n      if(!params) params = {};\n      this.router.navigate(['/resetpassword']);\n    }\n\n  async loginUser(credentials): Promise<void> {\n    this.authService.doLogin(credentials).then(\n      () => {\n        this.router.navigate(['/home']);\n      },\n      async error => {\n        const alert = await this.alertCtrl.create({\n          message: error.message,\n          buttons: [{ text: 'Ok', role: 'cancel' }],\n        });\n        await alert.present();\n      }\n    );\n  }\n\n  ngOnInit() {\n    \n    // firebase.auth().onAuthStateChanged(function(user) {\n    //   if (user) {\n    //     this.router.navigate(['/home']); //to the page where user navigates after login\n    //     // User is signed in.\n      \n    //   } else {\n    //     // this.router.navigate(['/login']); // to the login page as user is not logged in\n    //     // No user is signed in.\n\n    //   }\n    //   });\n\n  }\n\n}\n"],"sourceRoot":"webpack:///","file":"login-login-module-es5.js"}