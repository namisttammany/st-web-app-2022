let DocumentsService = class DocumentsService {
    constructor(http, db) {
        this.http = http;
        this.db = db;
        this.filesList = this.db.list('files', ref => ref.orderByChild('files'));
    }
    getFiles() {
        return this.filesList;
    }
};
