      function ResourceListService(db) {
        _classCallCheck(this, ResourceListService);

        this.db = db;
        this.resourceListRef = this.db.list('resource', function (ref) {
          return ref.orderByChild('ProgramName');
        });
        this.cityList = this.db.list('cities', function (ref) {
          return ref.orderByChild('city');
        });
        this.categoryList = this.db.list('categories', function (ref) {
          return ref.orderByChild('category');
        });
      }

      _createClass(ResourceListService, [{
        key: "getResourceList",
